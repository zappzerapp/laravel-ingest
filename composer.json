{
  "name": "zappzerapp/laravel-ingest",
  "version": "v0.3.0",
  "description": "A configuration-driven data import framework for Laravel",
  "keywords": [
    "laravel",
    "import",
    "csv",
    "excel",
    "data",
    "ingest",
    "etl"
  ],
  "homepage": "https://github.com/zappzerapp/laravel-ingest",
  "license": "AGPL-3.0-only",
  "authors": [
    {
      "name": "Robin Kopp",
      "email": "robin@zebra-fruit.de"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-fileinfo": "*",
    "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
    "illuminate/database": "^10.0 || ^11.0 || ^12.0",
    "illuminate/filesystem": "^10.0 || ^11.0 || ^12.0",
    "illuminate/http": "^10.0 || ^11.0 || ^12.0",
    "illuminate/queue": "^10.0 || ^11.0 || ^12.0",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0",
    "illuminate/validation": "^10.0 || ^11.0 || ^12.0",
    "laravel/serializable-closure": "^1.3",
    "spatie/simple-excel": "^3.2"
  },
  "require-dev": {
    "larastan/larastan": "^2.11",
    "laravel/pint": "^1.13",
    "league/flysystem-ftp": "^3.0",
    "orchestra/testbench": "^8.0 || ^9.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "roave/security-advisories": "dev-latest"
  },
  "suggests": {
    "league/flysystem-ftp": "Required to use the FTP source handler.",
    "league/flysystem-sftp-v3": "Required to use the SFTP source handler."
  },
  "autoload": {
    "psr-4": {
      "LaravelIngest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelIngest\\Tests\\": "tests/",
      "LaravelIngest\\Database\\Factories\\": "database/factories/"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan --memory-limit=1024M analyse",
    "lint": "vendor/bin/pint",
    "lint:check": "vendor/bin/pint --test",
    "test": "vendor/bin/pest",
    "test:coverage": "vendor/bin/pest --coverage",
    "docker:test": "docker compose exec -T app composer test",
    "docker:coverage": "docker compose exec -T app composer test:coverage",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-analyzer": true,
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelIngest\\IngestServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
